# interactive
docker run -it -v ${PWD}/config:/config -v ${PWD}/scripts:/code -v ${PWD}/logs:/logs -v ${PWD}/graphml:/graphml -v ${PWD}/output:/output --name pysbml4j_script_runner --rm pysbml4j_script_runner:0.1

# detached
docker run -d -v ${PWD}/config:/config -v ${PWD}/scripts:/code -v ${PWD}/logs:/logs -v ${PWD}/graphml:/graphml -v ${PWD}/output:/output --name pysbml4j_script_runner --rm pysbml4j_script_runner:0.1
